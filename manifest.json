{
    "name": "Leetcode Performance Tracker",
    "version": "1.0.0",
    "description": "Track leetcode performace",
    "manifest_version": 3,
    "author": "Venky",
    "action":{
        "default_popup": "front-end/HTML/index.html",
        "default_title": "Timer"
    },
    "background":{
        "service_worker": "front-end/JS/background.js"
    },
    "host_permissions": ["http://127.0.0.1:5000/*"],
    "permissions": [
        "background",
        "tabs"
    ] 
}